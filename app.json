{
  "expo": {
    "name": "Fairwithhair",
    "description": "This project is really really good.",
    "slug": "Fairwithhair",
    "privacy": "unlisted",
    "sdkVersion": "30.0.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/icon.png",
    "loading": {
      "icon": "./assets/icon.png",
      "hideExponentText": false
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "packagerOpts": {
      "assetExts": [
        "ttf",
        "mp4",
        "otf",
        "xml"
      ]
    },
    "android": {
      "package": "com.haircut.io"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.haircut.io"
    }
  }